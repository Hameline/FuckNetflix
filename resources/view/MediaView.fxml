<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="531.0" prefWidth="733.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="privatemovie.gui.controller.MediaViewController">
   <children>
      <ProgressBar fx:id="progressBar" layoutX="14.0" layoutY="493.0" prefHeight="18.0" prefWidth="705.0" progress="0.0" />
      <Button fx:id="playPauseBtn" layoutX="341.0" layoutY="452.0" mnemonicParsing="false" onAction="#handlePlayPause" text="Play/Pause" />
      <Button fx:id="stopBtn" layoutX="424.0" layoutY="452.0" mnemonicParsing="false" onAction="#handleStop" text="Stop" />
      <Slider fx:id="volumeSlider" layoutX="14.0" layoutY="458.0" />
      <BorderPane layoutX="14.0" layoutY="14.0" prefHeight="426.0" prefWidth="705.0">
         <center>
            <MediaView fx:id="movieView" fitHeight="200.0" fitWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
